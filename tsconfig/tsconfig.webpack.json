{
	"compilerOptions": {
		"target": "ES5",
		"module": "CommonJS",
		"allowJs": true,
		"removeComments": true,
		"jsx": "react",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"noImplicitAny": false,
		"strict": true,
		"resolveJsonModule": true,
		"strictNullChecks": true,
		"sourceMap": true,
		"skipLibCheck": true
	},
	"include": ["../src/app/**/*"]
}
