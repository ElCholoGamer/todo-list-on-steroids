{
	"compilerOptions": {
		"target": "ES5",
		"module": "CommonJS",
		"outDir": "../dist",
		"allowJs": true,
		"removeComments": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"strict": true,
		"noImplicitAny": true,
		"skipLibCheck": true,
		"strictNullChecks": true
	},
	"include": ["../src/server/**/*"]
}
